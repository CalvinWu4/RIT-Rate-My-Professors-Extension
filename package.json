{
  "name": "rit-rate-my-professor-extension",
  "version": "2.0.1",
  "description": "Shows the Rate My Professors rating of RIT professors while searching for classes on Tiger Center and CSH Schedule Maker.",
  "author": "Calvin Wu",
  "dependencies": {
    "@popperjs/core": "^2.8.6",
    "arrive": "^2.4.1",
    "papaparse": "^5.3.0",
    "tippy.js": "^6.3.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "filemanager-webpack-plugin": "^8.0.0",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "start": "webpack --config webpack.config.js",
    "release": "./build.sh",
    "build": "./build.sh keep-build",
    "test": "./build.sh keep-build && npm run test-all",
    "lint": "eslint src",
    "test-all": "npm run test-chrome && npm run test-ff",
    "test-ff": "web-ext lint --source-dir build/firefox",
    "test-chrome": "web-ext lint --source-dir build/chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CalvinWu4/RIT-Rate-My-Professor-Extension.git"
  },
  "bugs": {
    "url": "https://github.com/CalvinWu4/RIT-Rate-My-Professor-Extension/issues"
  },
  "homepage": "https://github.com/CalvinWu4/RIT-Rate-My-Professor-Extension#readme"
}
