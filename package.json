{
  "name": "rit-rate-my-professor-extension",
  "version": "2.0.1",
  "description": "Shows the Rate My Professors rating of RIT professors while searching for classes on Tiger Center and CSH Schedule Maker.",
  "author": "Calvin Wu",
  "private": true,
  "dependencies": {
    "arrive": "^2.4.1",
    "papaparse": "^5.3.0",
    "tippy.js": "^6.3.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "web-ext": "^7.6.2",
    "web-ext-plugin": "^2.7.0",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "clean": "rm -rf build/*; rm -rf dist/*",
    "build": "NODE_ENV=production webpack --config webpack.config.mjs",
    "build-dev": "NODE_ENV=development webpack --config webpack.config.mjs",
    "test": "npm run build && web-ext lint --source-dir build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CalvinWu4/RIT-Rate-My-Professor-Extension.git"
  },
  "bugs": {
    "url": "https://github.com/CalvinWu4/RIT-Rate-My-Professor-Extension/issues"
  },
  "homepage": "https://github.com/CalvinWu4/RIT-Rate-My-Professor-Extension#readme"
}
